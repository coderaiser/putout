{
  "name": "putout",
  "version": "1.1.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "find and remove unused variables",
  "homepage": "http://github.com/coderaiser/putout",
  "main": "lib/putout.js",
  "bin": {
    "putout": "bin/putout.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/putout.git"
  },
  "scripts": {
    "test": "tape 'test/*.js'",
    "watch:test": "nodemon -w lib -w test -x \"npm test\"",
    "watch:example": "nodemon -w lib -w test -x \"node remove-unused\"",
    "lint": "eslint lib test --ignore-pattern test/fixture",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@babel/traverse": "^7.1.6",
    "chalk": "^2.4.1",
    "recast": "^0.16.1"
  },
  "keywords": [
    "ast",
    "lint",
    "variable",
    "unused"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-node": "^8.0.0",
    "nodemon": "^1.14.12",
    "nyc": "^13.0.1",
    "redrun": "^7.0.2",
    "tape": "^4.8.0",
    "try-catch": "^2.0.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

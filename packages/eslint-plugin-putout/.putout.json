{
    "rules": {
        "putout": "off",
        "regexp/apply-starts-with": "off",
        "eslint/apply-ignores": "off",
        "eslint/add-putout": "off",
        "eslint/convert-file-to-array": "on",
        "eslint-plugin/update-ecma-version": ["on", {
            "ecmaVersion": 2025
        }]
    },
    "plugins": [
        "eslint-plugin"
    ],
    "match": {
        "eslint-fixture": {
            "esm": "off",
            "nodejs": "off",
            "nodejs/convert-commonjs-to-esm": "off",
            "apply-template-literals": "off",
            "nodejs/strict-mode": "off",
            "remove-empty": "off",
            "nodejs/remove-process-exit": "off",
            "typescript/remove-unused-types": "off",
            "remove-unused-expressions": "off",
            "arguments": "off",
            "variables/remove-useless-rename": "off",
            "variables/remove-useless-declarations": "off",
            "variables/remove-unused": "off",
            "spread": "off",
            "return/remove-useless": "off",
            "variables/remove-unreferenced": "off",
            "remove-unreachable-code": "off",
            "tape/remove-skip": "off",
            "tape/remove-only": "off",
            "remove-console": "off",
            "remove-debugger": "off",
            "typescript/remove-useless-types-from-constants": "off",
            "for-of/for": "off",
            "for-of/remove-useless": "off",
            "for-of/remove-unused-variables": "off",
            "maybe/noop": "off"
        }
    },
    "ignore": ["*.md", "eslint-fixture/.filesystem.json"]
}

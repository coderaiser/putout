{
    "rules": {
        "no-useless-return": "off"
    },
    "overrides": [{
        "files": ["*.md{js}"],
        "rules": {
            "n/no-deprecated-api": "off",
            "no-implicit-coercion": "off",
            "no-extra-boolean-cast": "off"
        }
    }],
    "extends": [
        "plugin:n/recommended",
        "plugin:putout/recommended"
    ],
    "plugins": [
        "putout",
        "n"
    ],
    "ignorePatterns": ["**/fixture"]
}

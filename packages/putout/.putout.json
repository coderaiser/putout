{
    "match": {
        ".madrun.js": {
            "madrun/add-madrun-to-lint": "off"
        },
        "test/putout.js": {
            "apply-shorthand-properties": [{
                "ignore": "plugin"
            }]
        }
    },
    "rules": {
        "putout": "off",
        "babel/transform-inline-consecutive-adds": "off",
        "jscodeshift/async-await-codemod/async-await": "off"
    },
    "plugins": [
        "babel/transform-inline-consecutive-adds",
        "jscodeshift/async-await-codemod/async-await"
    ]
}
